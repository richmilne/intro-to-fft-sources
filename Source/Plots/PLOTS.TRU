!SYSTEM FOR OPERATING PROGRAMS PLOT1 THRU PLOT8.!N MORRISON, DECEMBER 1993, CAPE TOWN, SOUTH AFRICA.  LIBRARY "PLOTSUB1.TRC"  LIBRARY "PLOTSUB2.TRC"  LIBRARY "PLOTSUB3.TRC"  LIBRARY "PLOTSUB4.TRC"  LIBRARY "PLOTSDOS.TRC"!  LIBRARY "StrLib*", "TabLib*", "TrueWindows-buttons*", "System*", "MacTools*"  LIBRARY "c:\tbgold\tblibs\strlib.trc"  LIBRARY "c:\tbgold\tblibs\truectrl.trc"  LIBRARY "c:\tbgold\tblibs\truedial.trc"  LIBRARY "c:\fftsou~1\LIBINFO.TRC"  DECLARE DEF EVAL  LET COLOR$ = "BLACK/WHITE"  PUBLIC window1  PUBLIC window2  CALL STARTUP			!( WINDOW1 )  DO    LET SHOAD = 1    DO      LET MMENU = 0      IF SHOAD = 1 THEN        CALL ADV           !ADVERTISEMENT FRAME        LET SHOAD = 0      END IF      CALL PAINTMAINMENU( OPTION$ )      IF OPTION$ = "A" THEN CALL PLOT2( COLOR$ )      IF OPTION$ = "B" THEN CALL PLOT3( COLOR$ )      IF OPTION$ = "C" THEN CALL PLOT4( COLOR$ )      IF OPTION$ = "D" THEN CALL PLOT5( COLOR$ )      IF OPTION$ = "E" THEN CALL PLOT6( COLOR$ )      IF OPTION$ = "F" THEN CALL PLOT7( COLOR$ )      IF OPTION$ = "G" THEN CALL PLOT1( COLOR$ )      IF OPTION$ = "H" THEN CALL PLOT8( COLOR$ )    LOOP UNTIL OPTION$ = "Q"    CALL BOX107( WXY$ )  LOOP UNTIL WXY$ = "Y"  CALL SHUTDOWNEND!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!