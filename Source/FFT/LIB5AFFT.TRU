 MMENU, HJWFLAG )1340     ELSE IF Z2$ = "B" OR Z2$ = "D" THEN1350       CALL CHANGEDATA   !( N, NU, XRE(), XIM(), X2RE(), X2IM(), FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), IFFTRE(), IFFTIM(), TEMPRE(), TEMPIM(), TEMP2RE(), TEMP2IM(), XFLAG, X2FLAG, REALFLAGX, FFTFLAG, FFT2FLAG, SYMMFLAG, IFFTFLAG, CONVFLAG, MODE$, TYPE$, VAR$, T, MMENU, HJWFLAG )1360     END IF13701380     IF LOOPING = 1 THEN1390       LET LOOPING = 01400       IF LOOPUP2 = 1 OR CYCLE = 1 OR LOOPUP3 = 1 THEN1410         IF LOOPUP2 = 1 OR CYCLE = 1 THEN1420           LET LOOPUP2 = 01430           LET CYCLE = 01440         END IF1450         LET MMENU = 01460         GOTO 11301470       END IF1480     END IF1490   END IF15001510 !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!1520 SUB CHANGEDATA15301540   LET SUB = 215501560   IF (VAR$ = "X" AND XFLAG = 0 AND X2FLAG = 0) OR (VAR$ = "FFT" AND FFTFLAG = 0 AND FFT2FLAG = 0) THEN1570     CALL BOX33( VAR$ )  !ALL VALUES ARE ZERO... CANNOT MAKE CHANGES1580     LET LOOPING = 11590     LET LOOPUP2 = 11600     LET MMENU = 11610   END IF16201630   IF MMENU = 0 THEN1640     LET LOOPUP3 = 11650     LET MULTT = 11660     IF VAR$ = "FFT" THEN1670       IF MODE$ = "SAMP" AND TYPE$ = "PER" THEN1680         LET MULTT = N1690       ELSE IF MODE$ = "SAMP" AND TYPE$ = "PUL" THEN1700         LET MULTT = N/T1710       END IF1720     END IF17301740     IF HJWFLAGCLEARED = 1 THEN1750       LET HJWFLAGCLEARED = 01760       LET HJWFLAG = 11770     END IF17801790     !DISPLAY31800     CALL BOX32( N, VAR$, Z1$ )  !CHANGE X, X2 OR BOTH,  (F, F2 OR BOTH)18101820     IF Z1$ = "Q" THEN1830       LET LOOPING = 11840       LET LOOPUP2 = 11850       LET LOOPUP3 = 01860       GOTO 506018701880     ELSE IF Z1$ = "X" OR Z1$ = "F" OR Z1$ = "B" THEN1890       IF VAR$ = "X" THEN1900         MAT TEMPRE = XRE1910         MAT TEMPIM = XIM1920       ELSE IF VAR$ = "FFT" THEN1930         MAT TEMPRE = FFTRE1940         MAT TEMPIM = FFTIM1950       END IF1960       LET TOT1 = 01970       LET VEX = 1    !PRIMARY VECTOR19801990           !DISPLAY42000       CALL BOX34( N, VAR$, Z2$, VEX ) !CHANGE REAL, IMAG OR BOTH20102020       IF Z2$ = "Q" THEN2030         GOTO 18002040       ELSE IF Z2$ = "R" OR Z2$ = "I" OR Z2$ = "B" THEN2050         LET K = 02060         LET NEWK = 020702080         DO2090               !DISPLAY52100           CALL BOX35( N, NEWK, VEX, Z3$, Z2$, VAR$ )  !USE OR SPECIFY K21102120           IF Z3$ = "Q" THEN2130             GOTO 20002140           ELSE IF Z3$ = "U" OR Z3$ = "S" THEN2150             IF Z3$ = "S" THEN21602170               CALL BOX117( NEWK, FLAG ) !SPECIFY K21802190               IF FLAG = 1 THEN2200                 GOTO 20802210               ELSE2220                 LET K = MOD(NEWK,N)2230               END IF2240             END IF22502260             IF Z2$ = "R" OR Z2$ = "I" OR Z2$ = "B" THEN2270               LET MESSAGE$ = "k = " & STR$(NEWK)2280               IF Z2$ = "R" OR Z2$ = "B" THEN2290                 IF VAR$ = "X" THEN2300                   LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  XRE(" & STR$(NEWK) & ")  IS SHOWN BELOW"2310                   LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"2320                   LET VALL = TEMPRE(K)2330                 ELSE IF VAR$ = "FFT" THEN2340                   LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  FRE("& STR$(NEWK) & ")  IS SHOWN BELOW"2350                   LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"2360                   LET VALL = TEMPRE(K)/MULTT2370                 END IF2380                 LET SHOWFLAG = 123902400                 CALL INPUTVALL( MESSAGE$, VALL, SHOWFLAG, FLAG )24102420                 LET SHOWFLAG = 02430                 IF FLAG = 1 THEN2440                   GOTO 20802450                 ELSE2460                   LET REAL = VALL*MULTT2470                 END IF24802490                 IF REAL <> TEMPRE(K) THEN2500                   LET TOT1 = TOT1 + 12510                   LET TEMPRE(K) = REAL2520                 END IF2530               END IF25402550               IF Z2$ = "I" OR Z2$ = "B" THEN2560                 LET MESSAGE$ = "k = " & STR$(NEWK)2570                 IF VAR$ = "X" THEN2580                   LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  XIM(" & STR$(NEWK) & ")  IS SHOWN BELOW"2590                   LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"2600                   LET VALL = TEMPIM(K)2610                 ELSE IF VAR$ = "FFT" THEN2620                   LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  FIM(" & STR$(NEWK) & ")  IS SHOWN BELOW"2630                   LET VALL = TEMPIM(K)/MULTT2640                   LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"2650                 END IF2660                 LET SHOWFLAG = 126702680                 CALL INPUTVALL( MESSAGE$, VALL, SHOWFLAG, FLAG )26902700                 LET SHOWFLAG = 02710                 IF FLAG = 1 THEN2720                   GOTO 20802730                 ELSE2740                   LET IMAG = VALL*MULTT2750                 END IF27602770                 IF IMAG <> TEMPIM(K) THEN2780                   LET TOT1 = TOT1 + 12790                   LET TEMPIM(K) = IMAG2800                 END IF2810               END IF2820             END IF2830           END IF28402850           LET NEWK = NEWK+12860           LET K = MOD(NEWK,N)28702880         LOOP UNTIL Z3$ = "C"2890       END IF29002910       LET NUMBERR = 129202930           !DISPLAY62940       CALL BOX29(TOT1, TOT2, N, Z3$, NUMBERR, SUB) !YOU HAVE CREATED29502960       IF Z3$ = "Q" THEN2970         LET LOOPING = 12980         LET LOOPUP2 = 12990         GOTO 50603000       END IF3010     END IF3020   END IF30303040   IF MMENU = 0 THEN30503060     IF Z1$ = "X2" OR Z1$ = "F2" OR Z1$ = "B" THEN3070       IF VAR$ = "FFT" AND HJWFLAG = 1 THEN30803090         CALL BOX59( CRT$ ) !fft2 is locked31003110         IF CRT$ = "P" THEN3120           LET HJWFLAG = 03130           LET HJWFLAGCLEARED = 13140         ELSE IF CRT$ = "Q" THEN3150           LET LOOPING = 13160           LET LOOPUP3 = 13170           GOTO 50603180         END IF3190       END IF32003210       LET TOT2 = 032203230       IF VAR$ = "X" THEN3240         MAT TEMP2RE = X2RE3250         MAT TEMP2IM = X2IM3260       ELSE IF VAR$ = "FFT" THEN3270         MAT TEMP2RE = FFT2RE3280         MAT TEMP2IM = FFT2IM3290       END IF33003310       LET VEX = 2    !A SECONDARY VECTOR33203330           !DISPLAY73340       CALL BOX34( N, VAR$, Z2$, VEX )  !CHANGE REAL OR IMAG OR BOTH33503360       IF Z2$ = "Q" THEN3370         LET LOOPING = 13380         LET LOOPUP2 = 13390         GOTO 50603400       ELSE IF Z2$ = "R" OR Z2$ = "I" OR Z2$ = "B" THEN3410         LET K = 03420         LET NEWK = 03430         DO3440               !DISPLAY83450           CALL BOX35( N, NEWK, VEX, ZQ3$, Z2$, VAR$ )  !USE OR SPECIFY K34603470           IF ZQ3$ = "Q" THEN3480             GOTO 33103490           ELSE IF ZQ3$ = "U" OR ZQ3$ = "S" THEN3500             IF ZQ3$ = "S" THEN35103520                   !DISPLAY93530               CALL BOX117( NEWK, GOBACK )   !SPECIFY K35403550               IF GOBACK = 1 THEN3560                 GOTO 34203570               ELSE3580                 LET K = MOD(NEWK,N)3590               END IF36003610             END IF36203630             IF Z2$ = "R" OR Z2$ = "B" THEN3640               LET MESSAGE$ = "k = " & STR$(NEWK)3650               IF VAR$ = "X" THEN3660                 LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  X2RE(" & STR$(NEWK) & ")  IS SHOWN BELOW"3670                 LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"3680                 LET VALL = TEMP2RE(K)3690               ELSE IF VAR$ = "FFT" THEN3700                 LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  F2RE(" & STR$(NEWK) & ")  IS SHOWN BELOW"3710                 LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"3720                 LET VALL = TEMP2RE(K)/MULTT3730               END IF3740               LET SHOWFLAG = 137503760               CALL INPUTVALL( MESSAGE$, VALL, SHOWFLAG, FLAG )37703780               LET SHOWFLAG = 03790               IF FLAG = 1 THEN3800                 GOTO 34203810               ELSE3820                 LET REAL = VALL*MULTT3830               END IF38403850               IF REAL <> TEMP2RE(K) THEN3860                 LET TOT2 = TOT2 + 13870                 LET TEMP2RE(K) = REAL3880               END IF38903900             END IF39103920             IF Z2$ = "I" OR Z2$ = "B" THEN3930               LET MESSAGE$ = "k = " & STR$(NEWK)3940               IF VAR$ = "X" THEN3950                 LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  X2IM(" & STR$(NEWK) & ")  IS SHOWN BELOW"3960                 LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"3970                 LET VALL = TEMP2IM(K)3980               ELSE IF VAR$ = "FFT" THEN3990                 LET MESSAGE$ = MESSAGE$ & "|" & "|" & "THE CURRENT VALUE OF  F2IM(" & STR$(NEWK) & ")  IS SHOWN BELOW"4000                 LET MESSAGE$ = MESSAGE$ & "|" & "PLEASE ENTER THE REQUIRED VALUE"4010                 LET VALL = TEMP2IM(K)/MULTT4020               END IF4030               LET SHOWFLAG = 140404050               CALL INPUTVALL( MESSAGE$, VALL, SHOWFLAG, FLAG )40604070               LET SHOWFLAG = 04080               IF FLAG = 1 THEN4090                 GOTO 34204100               ELSE4110                 LET IMAG = VALL*MULTT4120               END IF41304140               IF IMAG <> TEMP2IM(K) THEN4150                 LET TOT2 = TOT2 + 14160                 LET TEMP2IM(K) = IMAG4170               END IF41804190             END IF4200           END IF42104220           LET NEWK = NEWK+14230           LET K = MOD(NEWK,N)42404250         LOOP UNTIL ZQ3$ = "C"4260       END IF4270       LET NUMBERR = 242804290       CALL BOX29(TOT1, TOT2, N, Z3$, NUMBERR, SUB) !YOU HAVE CREATED43004310       IF Z3$ = "Q" THEN4320         LET LOOPING = 14330         LET LOOPUP2 = 14340         GOTO 50604350       END IF4360     END IF4370   END IF43804390   PRINT TAB(1,1);4400   IF MMENU = 0 THEN4410     IF TOT1 + TOT2 > 0 THEN4420       IF VAR$ = "X" THEN4430         IF Z1$ = "X" OR Z1$ = "B" THEN4440           FOR K = 0 TO N - 14450             LET XRE(K) = TEMPRE(K)4460             LET XIM(K) = TEMPIM(K)4470             IF MOD(K,128) = 0 THEN PRINT "=";4480           NEXT K4490         END IF4500         IF Z1$ = "X2" OR Z1$ = "B" THEN4510           FOR K = 0 TO N - 14520             LET X2RE(K) = TEMP2RE(K)4530             LET X2IM(K) = TEMP2IM(K)4540           NEXT K4550           IF MOD(K,128) = 0 THEN PRINT "=";4560         END IF45704580         CALL TESTXDATA( N, XRE(), XIM(), X2RE(), X2IM(), TEMP2RE(), TEMP2IM(), XFLAG, X2FLAG, REALFLAGX, KLEEN$ )45904600         MAT FFTRE = 04610         MAT FFTIM = 046204630         IF HJWFLAG = 0 THEN4640           MAT FFT2RE = 04650           MAT FFT2IM = 04660         END IF46704680         MAT IFFTRE = 04690         MAT IFFTIM = 047004710         CALL TESTFFTDATA( N, FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), TEMP2RE(), TEMP2IM(), FFTFLAG, FFT2FLAG, SYMMFLAG, HJWFLAG, KLEEN$ )47204730         CALL TESTIFFTDATA( N, IFFTRE(), IFFTIM(), TEMP2RE(), TEMP2IM(), IFFTFLAG, REALFLAGIFFT, CONVFLAG, KLEEN$ )47404750         CALL BOX60( HJWFLAG, SUB, VAR$ )    !ALL VALUES HAVE BEEN ACCEPTED47604770       ELSE IF VAR$ = "FFT" THEN4780         IF Z1$ = "F" OR Z1$ = "B" THEN4790           FOR K = 0 TO N - 14800             LET FFTRE(K)  = TEMPRE(K)4810             LET FFTIM(K)  = TEMPIM(K)4820             IF MOD(K,128) = 0 THEN PRINT "=";4830           NEXT K4840         END IF4850         IF Z1$ = "F2" OR Z1$ = "B" THEN4860           FOR K = 0 TO N - 14870             LET FFT2RE(K) = TEMP2RE(K)4880             LET FFT2IM(K) = TEMP2IM(K)4890             IF MOD(K,128) = 0 THEN PRINT "=";4900           NEXT K4910         END IF49204930         CALL TESTFFTDATA( N, FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), TEMP2RE(), TEMP2IM(), FFTFLAG, FFT2FLAG, SYMMFLAG, HJWFLAG, KLEEN$ )49404950         MAT IFFTRE = 04960         MAT IFFTIM = 049704980         CALL TESTIFFTDATA( N, IFFTRE(), IFFTIM(), TEMP2RE(), TEMP2IM(), IFFTFLAG, REALFLAGIFFT, CONVFLAG, KLEEN$ )49905000         CALL BOX60( HJWFLAG, SUB, VAR$ )    !ALL VALUES HAVE BEEN ACCEPTED50105020       END IF5030     END IF5040   END IF50505060 END SUB !CHANGEDATA5070 !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!5080509051005110 !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!5120 SUB CREATEDATA51305140   DO5150     LET CYCLEUP = 05160     LET SUB = 15170     DO5180       LET TOT1 = 05190       LET GOROUND = 052005210       MAT TEMPRE = 05220       MAT TEMPIM = 052305240       DO5250         LET GOBACK2 = 05260         LET CYCLE = 052705280         CALL BOX31( N, VAR$, Z1$ )   !ONLY X, BOTH X & X2, ONLY F, ...52905300         IF Z1$ = "Q" THEN5310           LET LOOPING = 15320           LET CYCLE = 15330           EXIT DO5340         END IF53505360         CALL CLEARALL(N, XRE(), XIM(), X2RE(), X2IM(), FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), IFFTRE(), IFFTIM(), TEMPRE(), TEMPIM(), TEMP2RE(), TEMP2IM(), XFLAG, X2FLAG, REALFLAGX, FFTFLAG, FFT2FLAG, SYMMFLAG, IFFTFLAG, REALFLAGIFFT, CONVFLAG, HJWFLAG )53705380         LET NUMBERR = 1  !PRIMARY  X  OR  F53905400         CALL BOX28( VAR$, N, Z4$, NUMBERR )   !REAL, COMPLEX, QUIT54105420         IF Z4$ = "Q" THEN LET GOBACK2 = 154305440       LOOP WHILE GOBACK2 = 154505460       IF CYCLE = 0 THEN54705480 