SUB BOXXPOST(Z0$, MODE$ )

  DECLARE PUBLIC window1

  DIM MENU$(0:1,3)       !dimensioning for three menus, APPLE and X-POSTPROCS

  DIM CC$(0:1,3)         !required later for transposing

  DIM LIST$(1)             !to hold the ID list
  DIM LIST2$(1)             !to hold the ID list

  LET L = 1

  LET DIMFLAG = 0

  LET MENU$(0,3) = "INFO@I"
  LET MENU$(1,3) = "Info Page...@I"

  LET MENU$(0,1) = "XPOST MENU 1@X"

  LET DIMFLAG = 1
  LET MENU$(L,1) = "Copy  X  to  X2@X"
  LET LIST$(L) = "CX"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  X2  to  X@2"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CX2"
  LET L = L+1

  IF DIMFLAG = 1 THEN MAT REDIM MENU$(0:L,3)
  LET DIMFLAG = 1
  LET MENU$(L,1) = "Copy  X  to  F@F"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CXF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  F  to  X@t"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CFX"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  Y  to  X@Y"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CYX"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Add  X  to  X2@A"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "AX"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Add  X2  to  X@d"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "AX2"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:X2@S"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SW"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:X2  F:F2@w"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SWS"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:F@o"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SXF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:F  X2:F2@p"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SWO"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Quit @Q"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "Q"

 !------------------------------!

  LET M = 1
  MAT REDIM LIST2$(L)
  LET MENU$(0,2) = "XPOST MENU 2@P"

  LET MENU$(M,2) = "Multiply X by a constant@M"
  LET LIST2$(M) = "MX"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Multiply X2 by a constant@X"
  LET LIST2$(M) = "MX2"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Real-mult XRE & X2RE ---> XRE@R"
  LET LIST2$(M) = "RM"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Complex-mult X & X2 ---> X@C"
  LET LIST2$(M) = "CM"
  LET M = M+1

  IF MODE$ = "SAMP" THEN
    IF M > L THEN
      MAT REDIM MENU$(0:M,3)
      MAT REDIM LIST2$(M)
    END IF
    LET MENU$(M,2) = "Multiply X by exp(j wo t)@e"
    LET LIST2$(M) = "EXP"
    LET M = M+1
  END IF

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Set  XIM  to zero@I"
  LET LIST2$(M) = "ZI"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Set  XRE  to zero@z"
  LET LIST2$(M) = "ZR"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Impulse Sample X@S"
  LET LIST2$(M) = "SX"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Rect Sample X@R"
  LET LIST2$(M) = "RSX"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Clean X@a"
  LET LIST2$(M) = "CLX"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Clean X2@2"
  LET LIST2$(M) = "CLX2"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Quit @Q"
  LET LIST2$(M) = "Q"

  LET V = MAX(M,L)
  !
  !now transposing
  !

  MAT REDIM CC$(3,0:V)

  FOR I = 0 TO V
    FOR J = 1 TO 3
      LET CC$(J,I) = MENU$(I,J)
    NEXT J
  NEXT I

  MAT REDIM MENU$(3,0:V)
  MAT MENU$ = CC$

  WHEN ERROR IN
    CALL TC_Menu_GetText(window1, 1,0, text$)
  USE
    CALL TC_Menu_Set(window1, MENU$)
  END WHEN

!  CALL TC_Menu_Set(window1,menu$)       !remove current menus, install menu$

  DO
    LET CYCLEFLAG = 0

    DO           ! Here's our event loop

      CALL TC_Event(0,type1$,window,x1,x2)

      SELECT CASE type1$

      CASE "MENU"
        !
        ! Note what was selected
        !
        LET mitem$ = menu$(x1,x2)
        LET p = Pos(mitem$,"@")
        IF p > 0 then LET mitem$[p:maxnum] = ""   ! strip shortcuts
        LET mtitle$ = menu$(x1,0)

        IF X1 = 3 AND X2 = 1 THEN

!          CALL TC_Menu_Free(window1)
          CALL BOX138

          LET CYCLEFLAG = 1

        ELSE IF X1 = 1 THEN
          FOR K = 1 TO L
            IF X2 = K THEN
              LET Z0$ = LIST$(K)
            END IF
          NEXT K

        ELSE IF X1 = 2 THEN
          FOR K = 1 TO M
            IF X2 = K THEN
              LET Z0$ = LIST2$(K)
            END IF
          NEXT K
        END IF

        EXIT DO

      CASE "SINGLE"
        SOUND 500,.1
      CASE "DOUBLE"
        SOUND 500,.1

      CASE ELSE
          !
          ! Ignore other events
          !

      END SELECT
    LOOP
  LOOP WHILE CYCLEFLAG = 1

  IF Z0$ = "Q" THEN CALL TC_Menu_Free(window1)

!  CALL TC_Menu_Free(window1)

END SUB !BOXXPOST
