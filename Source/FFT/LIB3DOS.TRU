SUB BOXEP( MESSAGE$, QEZ$ )

  LET ICON = 2

  LET BUTTON$ = "MORE|QUIT"

  LET DEFAULT = 1

  CALL TD_Warn(MESSAGE$, BUTTON$, DEFAULT, RESULT)

  IF RESULT = 1 THEN
    LET QEZ$ = "C"
  ELSE IF RESULT = 2 THEN
    LET QEZ$ = "Q"
  END IF

