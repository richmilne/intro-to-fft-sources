      ELSE IF (COND = 1 AND RESULT = 2) OR ((COND = 2 OR COND = 3) AND RESULT = 3) OR (COND = 4 AND RESULT = 4) THEN
        LET SKIPOUT = 1
      END IF

      IF CYCLE = 0 THEN
        LET ORG$  = TORG$
        LET ORG2$ = TORG2$
      END IF

    LOOP WHILE CYCLE = 1
  LOOP UNTIL SKIPOUT = 1

  LET KILLSWITCHFLAG = 1

