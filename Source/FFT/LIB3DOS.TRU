!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
SUB BOXFPOST( Z0$, MODE$, TYPE$ )


  DECLARE PUBLIC window1
  DIM MENU$(0:1,3)       !dimensioning for three menus, APPLE and F-POSTPROCS

  DIM CC$(0:1,3)         !required later for transposing

  DIM LIST$(1)             !to hold the ID list
  DIM LIST2$(1)            !to hold the ID list

  LET L = 1

  LET DIMFLAG = 0

  LET MENU$(0,3) = "INFO@I"
  LET MENU$(1,3) = "Info Page...@I"

  LET MENU$(0,1) = "FPOST MENU 1@F"

  LET DIMFLAG = 1
  LET MENU$(L,1) = "Copy  F  to  F2@F"
  LET LIST$(L) = "CF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  F2  to  F@2"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CF2"
  LET L = L+1

  IF DIMFLAG = 1 THEN MAT REDIM MENU$(0:L,3)
  LET DIMFLAG = 1
  LET MENU$(L,1) = "Copy  F  to  X@X"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CFX"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  X  to  F@C"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CXF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Copy  FRE  to  FIM@R"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CFI"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Add  F  to  F2@A"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "AF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Add  F2  to  F@d"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "AF2"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  F:F2@S"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SW"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:X2  F:F2@w"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SWS"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:F@o"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SXF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  X:F  X2:F2@p"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SWO"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Swop  FRE:FIM@E"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "SWI"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Multiply F by a constant@M"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "MF"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Multiply F2 by a constant@u"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "MF2"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Complex-mult F & F2 ---> F@l"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "CM"
  LET L = L+1

  IF MODE$ = "SAMP" THEN
    MAT REDIM MENU$(0:L,3)
    LET MENU$(L,1) = "Multiply F by exp(j w tau)@j"
    MAT REDIM LIST$(L)
    LET LIST$(L) = "EXP"
    LET L = L+1
  END IF

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,3)
  LET MENU$(L,1) = "Quit @Q"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "Q"

!-----------------------------------!

  LET M = 1
  MAT REDIM LIST2$(L)
  LET MENU$(0,2) = "FPOST MENU 2@2"

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Conjugate  F@F"
  LET LIST2$(M) = "CJF"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Conjugate  F2@C"
  LET LIST2$(M) = "CJF2"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Shave  F@S"
  LET LIST2$(M) = "SH"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Low-pass filter  F@L"
  LET LIST2$(M) = "LPF"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "High-pass filter  F@H"
  LET LIST2$(M) = "HPF"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Set  FIM  to zero@I"
  LET LIST2$(M) = "ZI"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Set  FRE  to zero@R"
  LET LIST2$(M) = "ZR"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Backward-diff 1st@B"
  LET LIST2$(M) = "BD"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Forward-diff 1st@1"
  LET LIST2$(M) = "FD"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Central-diff 1st@d"
  LET LIST2$(M) = "CD"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Central-diff 2nd@n"
  LET LIST2$(M) = "CD2"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF MODE$ = "SAMP" THEN
    IF M > L THEN
      MAT REDIM MENU$(0:M,3)
      MAT REDIM LIST2$(M)
    END IF
    IF TYPE$ = "PER" THEN
      LET MENU$(M,2) = "Power@P"
    ELSE IF TYPE$ = "PUL" THEN
      LET MENU$(M,2) = "Energy@E"
    END IF
    LET LIST2$(M) = "EP"
    LET M = M+1
  END IF

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Sample  F@a"
  LET LIST2$(M) = "SF"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Power  F@o"
  LET LIST2$(M) = "PF"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Lock  F2@k"
  LET LIST2$(M) = "LF2"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
  END IF
  LET MENU$(M,2) = "@"
  LET M = M+1

  IF M > L THEN
    MAT REDIM MENU$(0:M,3)
    MAT REDIM LIST2$(M)
  END IF
  LET MENU$(M,2) = "Quit @Q"
  LET LIST2$(M) = "Q"

  LET V = MAX(M,L)
  !
  !now transposing
  !

  MAT REDIM CC$(3,0:V)

  FOR I = 0 TO V
    FOR J = 1 TO 3
      LET CC$(J,I) = MENU$(I,J)
    NEXT J
  NEXT I

  MAT REDIM MENU$(3,0:V)
  MAT MENU$ = CC$

  WHEN ERROR IN
    CALL TC_Menu_GetText(window1, 1,0, text$)
  USE
    CALL TC_Menu_Set(window1, MENU$)
  END WHEN

!  CALL TC_Menu_Set(window1,menu$)       !remove current menus, install menu$

  DO
    LET CYCLEFLAG = 0

    DO           ! Here's our event loop

      CALL TC_Event(0,type1$,window,x1,x2)

      SELECT CASE type1$

      CASE "MENU"
        !
        ! Note what was selected
        !
        LET mitem$ = menu$(x1,x2)
        LET p = Pos(mitem$,"@")
        IF p > 0 then LET mitem$[p:maxnum] = ""   ! strip shortcuts
        LET mtitle$ = menu$(x1,0)

        IF X1 = 3 AND X2 = 1 THEN

!          CALL TC_Menu_Free(window1)
          CALL BOX138

          LET CYCLEFLAG = 1

        ELSE IF X1 = 1 THEN
          FOR K = 1 TO L
            IF X2 = K THEN
              LET Z0$ = LIST$(K)
            END IF
          NEXT K

        ELSE IF X1 = 2 THEN
          FOR K = 1 TO M
            IF X2 = K THEN
              LET Z0$ = LIST2$(K)
            END IF
          NEXT K
        END IF

        EXIT DO

      CASE "SINGLE"
        SOUND 500,.1
      CASE "DOUBLE"
        SOUND 500,.1

      CASE ELSE

          !
          ! Ignore other events
          !

      END SELECT
    LOOP
  LOOP WHILE CYCLEFLAG = 1

  IF Z0$ = "Q" THEN CALL TC_Menu_Free(window1)
!  CALL TC_Menu_Free(window1)

END SUB !BOXFPOST
