      LET RWS = 1
    LOOP UNTIL RSB = 1

    IF ZUP$ = "T" THEN
      IF PULSESCALE = 1 THEN
        LET PULSESCALE = 2
      ELSE IF PULSESCALE = 2 THEN
        LET PULSESCALE = 1
