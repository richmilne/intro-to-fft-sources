SUB BOX17( MODE$, TYPE$, OPTION$, A$, XFLAG, FFTFLAG, X2FLAG, FFT2FLAG, IFFTFLAG, REALFLAGX, REALFLAGIFFT )

  CLEAR

  PRINT TAB(2,21);               "ษออออออออออออออออออออออออออออออออออออออป"
  PRINT TAB(3,21);               "บ         PRIMARY VECTOR PLOTS         บ"
  PRINT TAB(4,21);               "ฬออออออออออออออออออออออออออออออออออออออน"
  IF XFLAG = 1 THEN
    IF REALFLAGX = 0 THEN
      LET W = 1
      PRINT TAB(5,21);           "บ  REAL PART OF X ................. A  บ"
