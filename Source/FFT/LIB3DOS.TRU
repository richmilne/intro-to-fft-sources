SUB BOX17( OPTION$, MODE$, TYPE$, XFLAG, X2FLAG, REALFLAGX, FFTFLAG, FFT2FLAG, IFFTFLAG, REALFLAGIFFT )

  DECLARE PUBLIC window1

  DIM MENU$(0:1,2)       !dimensioning for two menus, APPLE and PRIMARY PLOTS
  DIM CC$(0:1,2)         !required later for transposing

  IF X2FLAG = 0 AND FFT2FLAG = 0 THEN
    DIM LIST$(1)             !to hold the primary ID list
    LET COL = 1
  ELSE IF X2FLAG <> 0 OR FFT2FLAG <> 0  THEN
    MAT REDIM MENU$(0:1,3)       !dimensioning for three menus, APPLE, PRIMARY and SECONDARY
    MAT REDIM CC$(3,0:1)         !required later for transposing
    DIM LIST2$(1)                  !to hold the secondary ID list
    LET COL = 2
  END IF

  LET MENU$(0,COL+1) = "INFO@I"
  LET MENU$(1,COL+1) = "Info Page...@I"

  LET L = 1                    !NOW CREATING PRIMARY PLOTS MENU

  LET DIMFLAG = 0

  MAT REDIM MENU$(0:L,COL+1)
  MAT REDIM LIST$(L)

  LET MENU$(0,1) = "PRIMARY PLOTS@P"

  IF XFLAG = 1 OR X2FLAG = 1 THEN
    LET DIMFLAG = 1

    IF REALFLAGX = 1 THEN

      LET MENU$(L,1) =   "X vector @X"
      LET LIST$(L) = "A"
      LET L = L+1

    ELSE IF REALFLAGX = 0 THEN

      LET MENU$(L,1) =   "Real part of X @X"
      LET LIST$(L) = "A"
      LET L = L+1

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) =   "Imag part of X @I"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "B"
      LET L = L+1

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) =  "Modulus of X @M"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "C"
      LET L = L+1

    END IF
  END IF

  IF FFTFLAG = 1 OR FFT2FLAG = 1 THEN
    IF DIMFLAG = 1 THEN
      MAT REDIM MENU$(0:L,COL+1)
    END IF
    LET DIMFLAG = 1

    LET MENU$(L,1) = "@"
    LET L = L+1

    MAT REDIM MENU$(0:L,COL+1)
    LET MENU$(L,1) =   "Real part of F @R"
    MAT REDIM LIST$(L)
    LET LIST$(L) = "D"
    LET L = L+1

    MAT REDIM MENU$(0:L,COL+1)
    LET MENU$(L,1) =   "Imag part of F @F"
    MAT REDIM LIST$(L)
    LET LIST$(L) = "E"
    LET L = L+1

    IF MODE$ = "SAMP" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Magnitude spectrum @d"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "F"
      LET L = L+1

    ELSE IF MODE$ = "DISC" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Modulus of F @d"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "F"
      LET L = L+1

    END IF

    IF MODE$ = "SAMP" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Phase spectrum @P"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "G"
      LET L = L+1

    ELSE IF MODE$ = "DISC" THEN
      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Phase of F @P"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "G"
      LET L = L+1

    END IF

    IF MODE$ = "SAMP" AND TYPE$ = "PER" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Power Spectrum @S"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "H"
      LET L = L+1

    ELSE IF MODE$ = "SAMP" AND TYPE$ = "PUL" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Energy Spectrum @S"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "H"
      LET L = L+1

    ELSE IF MODE$ = "DISC" THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Mod F Squared @S"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "H"
      LET L = L+1

    END IF
  END IF

  IF IFFTFLAG = 1 THEN
    IF DIMFLAG = 1 THEN
      MAT REDIM MENU$(0:L,COL+1)
    END IF

    LET MENU$(L,1) = "@"
    LET L = L+1

    IF REALFLAGIFFT = 1 THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Y vector @Y"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "I"
      LET L = L+1

    ELSE IF REALFLAGIFFT = 0 THEN

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Real part of Y @Y"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "I"
      LET L = L+1

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Imag part of Y @a"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "J"
      LET L = L+1

      MAT REDIM MENU$(0:L,COL+1)
      LET MENU$(L,1) = "Modulus of Y @o"
      MAT REDIM LIST$(L)
      LET LIST$(L) = "K"
      LET L = L+1

    END IF
  END IF

  MAT REDIM MENU$(0:L,COL+1)
  LET MENU$(L,1) = "@"
  LET L = L+1

  MAT REDIM MENU$(0:L,COL+1)
  LET MENU$(L,1) = "Toggle @T"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "L"
  LET L = L+1

  MAT REDIM MENU$(0:L,COL+1)
  LET MENU$(L,1) = "Quit @Q"
  MAT REDIM LIST$(L)
  LET LIST$(L) = "Q"

  IF COL = 2 THEN              !NOW CREATING SECONDARY PLOTS MENU

    MAT REDIM LIST2$(L)

    LET M = 1

    LET MENU$(0,2) = "SECONDARY PLOTS@S"

    IF X2FLAG = 1 THEN

      IF REALFLAGX = 1 THEN

        LET MENU$(M,2) =   "X2 vector @X"
        LET LIST2$(M) = "A2"
        LET M = M +1

      ELSE IF REALFLAGX = 0 THEN

        LET MENU$(M,2) =   "Real part of X2 @X"
        LET LIST2$(M) = "A2"
        LET M = M +1

        LET MENU$(M,2) =   "Imag part of X2 @I"
        LET LIST2$(M) = "B2"
        LET M = M +1

        LET MENU$(M,2) =  "Modulus of X2 @M"
        LET LIST2$(M) = "C2"
        LET M = M +1

      END IF
    END IF

    IF FFT2FLAG = 1 THEN

      LET MENU$(M,2) = "@"
      LET M = M +1

      LET MENU$(M,2) =   "Real part of F2 @R"
      LET LIST2$(M) = "D2"
      LET M = M +1

      LET MENU$(M,2) =   "Imag part of F2 @I"
      LET LIST2$(M) = "E2"
      LET M = M +1

      IF MODE$ = "SAMP" THEN

        LET MENU$(M,2) = "Magnitude spectrum @d"
        LET LIST2$(M) = "F2"
        LET M = M +1

      ELSE IF MODE$ = "DISC" THEN

        LET MENU$(M,2) = "Modulus of F2 @d"
        LET LIST2$(M) = "F2"
        LET M = M +1

      END IF

      IF MODE$ = "SAMP" THEN

        LET MENU$(M,2) = "Phase spectrum @P"
        LET LIST2$(M) = "G2"
        LET M = M +1

      ELSE IF MODE$ = "DISC" THEN
        LET MENU$(M,2) = "Phase of F2 @P"
        LET LIST2$(M) = "G2"
        LET M = M +1

      END IF

      IF MODE$ = "SAMP" AND TYPE$ = "PER" THEN

        LET MENU$(M,2) = "Power Spectrum @S"
        LET LIST2$(M) = "H2"
        LET M = M +1

      ELSE IF MODE$ = "SAMP" AND TYPE$ = "PUL" THEN

        LET MENU$(M,2) = "Energy Spectrum @S"
        LET LIST2$(M) = "H2"
        LET M = M +1

      ELSE IF MODE$ = "DISC" THEN

       LET MENU$(M,2) = "Mod F Squared @S"
        LET LIST2$(M) = "H2"
        LET M = M +1

      END IF

    END IF

    LET MENU$(M,2) = "@"
    LET M = M +1

    LET MENU$(M,2) = "Toggle @T"
    LET LIST2$(M) = "L"
    LET M = M +1

    LET MENU$(M,2) = "Quit @Q"
    LET LIST2$(M) = "Q"

  END IF

  !
  !now transposing
  !

  MAT REDIM CC$(COL+1,0:L)

  FOR I = 0 TO L
    FOR J = 1 TO COL+1
      LET CC$(J,I) = MENU$(I,J)
    NEXT J
  NEXT I

  MAT REDIM MENU$(COL+1,0:L)
  MAT MENU$ = CC$

  WHEN ERROR IN
    CALL TC_Menu_GetText(window1, 1,0, text$)
  USE
    CALL TC_Menu_Set(window1, MENU$)
  END WHEN

!  CALL TC_Menu_Set(window1,menu$)       !remove current menus, install menu$

  DO
    LET CYCLEFLAG = 0

    DO              ! Here's our event loop

      CALL TC_Event(0,type1$,window,x1,x2)

      SELECT CASE type1$

      CASE "MENU"
        !
        ! Note what was selected
        !
        LET mitem$ = menu$(x1,x2)
        LET p = Pos(mitem$,"@")
        IF p > 0 then LET mitem$[p:maxnum] = ""   ! strip shortcuts
        LET mtitle$ = menu$(x1,0)

        IF X1 = COL+1 AND X2 = 1 THEN

          CALL BOX138

          LET CYCLEFLAG = 1
          EXIT DO

        ELSE IF X1 = 1 THEN
          FOR K = 1 TO L
            IF X2 = K THEN
              LET OPTION$ = LIST$(K)
            END IF
          NEXT K
          EXIT DO

        ELSE IF X1 = 2 AND COL = 2 THEN
          FOR K = 1 TO L
            IF X2 = K THEN
              LET OPTION$ = LIST2$(K)
            END IF
          NEXT K
          EXIT DO
        END IF

      CASE "SINGLE"
        SOUND 500,.1
      CASE "DOUBLE"
        SOUND 500,.1

      CASE ELSE
          !
          ! Ignore other events
          !

      END SELECT
    LOOP

  LOOP WHILE CYCLEFLAG = 1

  IF OPTION$ = "Q" THEN CALL TC_Menu_Free(window1)
!  CALL TC_Menu_Free(window1)

END SUB !BOX17
