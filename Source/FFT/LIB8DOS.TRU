EXTERNAL             !LIB8DOS!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB TESTXDATA( N, XRE(), XIM(), X2RE(), X2IM(), TEMP2RE(), TEMP2IM(), XFLAG, X2FLAG, REALFLAGX, KLEEN$ )  LET XFLAG  = 0  LET X2FLAG = 0  LET REALFLAGX  = 1  PRINT "=";  MAT TEMP2RE = XRE  MAT TEMP2IM = XIM  IF KLEEN$ = "ONE" THEN CALL CLEANUP( N, TEMP2RE(), TEMP2IM() )  FOR K = 0 TO N - 1    IF TEMP2RE(K) <> 0 OR TEMP2IM(K) <> 0 THEN LET XFLAG = 1    IF TEMP2IM(K) <> 0 THEN LET REALFLAGX = 0    IF MOD(K,2048) = 0 THEN PRINT "=";  NEXT K  PRINT "=";  MAT TEMP2RE = X2RE  MAT TEMP2IM = X2IM  IF KLEEN$ = "ONE" THEN CALL CLEANUP( N, TEMP2RE(), TEMP2IM() )  FOR K = 0 TO N - 1    IF TEMP2RE(K) <> 0 OR TEMP2IM(K) <> 0 THEN LET X2FLAG = 1    IF TEMP2IM(K) <> 0 THEN LET REALFLAGX = 0    IF MOD(K,2048) = 0 THEN PRINT "=";  NEXT KEND SUB !TESTXDATA!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB TESTFFTDATA( N, FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), TEMP2RE(), TEMP2IM(), FFTFLAG, FFT2FLAG, SYMMFLAG, HJWFLAG, KLEEN$ )  LET SYMMFLAG = 1  LET FFTFLAG  = 0  !TESTING FFT  PRINT "=";  MAT TEMP2RE = FFTRE  MAT TEMP2IM = FFTIM  IF KLEEN$ = "ONE" THEN CALL CLEANUP( N, TEMP2RE(), TEMP2IM() )  CALL TESTFFT( TEMP2RE(), TEMP2IM(), FFTFLAG, SYMMFLAG )  !TESTING FFT2  PRINT "=";  LET FFT2FLAG = 0  MAT TEMP2RE = FFT2RE  MAT TEMP2IM = FFT2IM  IF KLEEN$ = "ONE" THEN CALL CLEANUP( N, TEMP2RE(), TEMP2IM() )  CALL TESTFFT( TEMP2RE(), TEMP2IM(), FFT2FLAG, SYMMFLAG )  IF FFT2FLAG = 0 THEN LET HJWFLAG = 0!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB TESTFFT( TEMP2RE(), TEMP2IM(), AFLAG, BFLAG )  FOR K = 0 TO N - 1    IF TEMP2RE(K) <> 0 OR TEMP2IM(K) <> 0 THEN LET AFLAG = 1    IF MOD(K,2048) = 0 THEN PRINT "=";  NEXT K  IF N/2 = INT(N/2) THEN       ! N is even    FOR K = 1 TO INT(N/2)      IF TEMP2RE(K) <>  TEMP2RE(N - K) THEN LET BFLAG = 0      IF TEMP2IM(K) <> -TEMP2IM(N - K) THEN LET BFLAG = 0      IF MOD(K,2048) = 0 THEN PRINT "";    NEXT K    IF TEMP2IM(0)   <> 0 THEN LET BFLAG = 0    IF TEMP2IM(N/2) <> 0 THEN LET BFLAG = 0  ELSE IF N/2 <> N/2 THEN      ! N is odd    FOR K = 1 TO INT(N/2)      IF TEMP2RE(K) <> 0 THEN        IF ABS(TEMP2RE(K) - TEMP2RE(N - K))/TEMP2RE(K) > 1E-6 THEN LET BFLAG = 0      ELSE IF TEMP2RE(K) = 0 THEN        IF ABS(TEMP2RE(K) - TEMP2RE(N - K)) > 1E-6 THEN LET BFLAG = 0      END IF      IF TEMP2IM(K) <> 0 THEN        IF ABS(TEMP2IM(K) + TEMP2IM(N - K))/TEMP2IM(K) > 1E-6 THEN LET BFLAG = 0      ELSE IF TEMP2IM(K) = 0 THEN        IF ABS(TEMP2IM(K) + TEMP2IM(N - K)) > 1E-6 THEN LET BFLAG = 0      END IF      IF MOD(K,2048) = 0 THEN PRINT "";    NEXT K    IF ABS(TEMP2IM(0)) > 1E-6 THEN LET BFLAG = 0  END IFEND SUB !TESTFFT!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!END SUB !TESTFFTDATA!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB TESTIFFTDATA( N, IFFTRE(), IFFTIM(), TEMP2RE(), TEMP2IM(), IFFTFLAG, REALFLAGIFFT, CONVFLAG, KLEEN$ )  LET IFFTFLAG  = 0  LET REALFLAGIFFT = 1  PRINT "=";  MAT TEMP2RE = IFFTRE  MAT TEMP2IM = IFFTIM  IF KLEEN$ = "ONE" THEN CALL CLEANUP( N, TEMP2RE(), TEMP2IM() )  FOR K = O TO N - 1    IF TEMP2RE(K) <> 0 OR TEMP2IM(K) <> 0 THEN LET IFFTFLAG = 1    IF TEMP2IM(K) <> 0 THEN LET REALFLAGIFFT = 0    IF MOD(K,2048) = 0 THEN PRINT "=";  NEXT K  IF IFFTFLAG = 0 THEN LET CONVFLAG = 0END SUB !TESTIFFTDATA!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB CLEARALL(N, XRE(), XIM(), X2RE(), X2IM(), FFTRE(), FFTIM(), FFT2RE(), FFT2IM(), IFFTRE(), IFFTIM(), TEMPRE(), TEMPIM(), TEMP2RE(), TEMP2IM(), XFLAG, X2FLAG, REALFLAGX, FFTFLAG, FFT2FLAG, SYMMFLAG, IFFTFLAG, REALFLAGIFFT, CONVFLAG, HJWFLAG )  PRINT TAB(1,1);  MAT XRE     = 0  MAT XIM     = 0  PRINT "=";  MAT FFTRE   = 0  MAT FFTIM   = 0  PRINT "=";  MAT IFFTRE  = 0  MAT IFFTIM  = 0  PRINT "=";  MAT TEMPRE  = 0  MAT TEMPIM  = 0  PRINT "=";  MAT TEMP2RE = 0  MAT TEMP2IM = 0  MAT X2RE = 0  MAT X2IM = 0  PRINT "=";  IF HJWFLAG = 0 THEN    MAT FFT2RE = 0    MAT FFT2IM = 0    LET FFT2FLAG = 0    PRINT "=";  END IF  LET XFLAG         = 0  LET X2FLAG        = 0  LET REALFLAGX     = 1  LET FFTFLAG       = 0  LET SYMMFLAG      = 1  LET IFFTFLAG      = 0  LET REALFLAGIFFT  = 1  LET CONVFLAG      = 0END SUB !CLEARALL!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB CLEANUP( N, TEMPRE(), TEMPIM() )  LET MAXRE = 0  LET MAXIM = 0  FOR I = 0 TO N - 1    LET MAXRE = MAX( MAXRE, ABS(TEMPRE(I)) )    LET MAXIM = MAX( MAXIM, ABS(TEMPIM(I)) )    IF (MOD(I,1024) = 0 AND N >= 1024 AND N <= 4096) OR (MOD(I,4096) = 0 AND N > 4096) THEN PRINT "=";  NEXT I  LET MAXALL = MAX( MAXRE, MAXIM )  IF MAXRE < 1E-7*MAXALL THEN    FOR I = 0 TO N - 1      LET TEMPRE(I) = 0      IF (MOD(I,1024) = 0 AND N >= 1024 AND N <= 4096) OR (MOD(I,4096) = 0 AND N > 4096) THEN PRINT "=";    NEXT I  ELSE    FOR I = 0 TO N - 1      IF ABS(TEMPRE(I)) < 1E-7*MAXRE THEN LET TEMPRE(I) = 0      IF (MOD(I,1024) = 0 AND N >= 1024 AND N <= 4096) OR (MOD(I,4096) = 0 AND N > 4096) THEN PRINT "=";    NEXT I  END IF  IF MAXIM < 1E-7*MAXALL THEN    FOR I = 0 TO N - 1      LET TEMPIM(I) = 0      IF (MOD(I,1024) = 0 AND N >= 1024 AND N <= 4096) OR (MOD(I,4096) = 0 AND N > 4096) THEN PRINT "=";    NEXT I  ELSE    FOR I = 0 TO N - 1      IF ABS(TEMPIM(I)) < 1E-7*MAXIM THEN LET TEMPIM(I) = 0      IF (MOD(I,1024) = 0 AND N >= 1024 AND N <= 4096) OR (MOD(I,4096) = 0 AND N > 4096) THEN PRINT "=";    NEXT I  END IFEND SUB !CLEANUP!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB BOX54( FN$, Z0$, N, CONCORFLAG )  LET ICON = 2  IF FN$ = "A" THEN    LET MESSAGE$ = "RUN ANALYSIS ?"  ELSE IF FN$ = "S" THEN    LET MESSAGE$ = "RUN SYNTHESIS ?"  ELSE IF FN$ = "C" THEN    IF CONCORFLAG = 1 THEN      LET MESSAGE$ = "RUN CONVOLUTION ?"    ELSE IF CONCORFLAG = 0 THEN      LET MESSAGE$ = "RUN CORRELATION ?"    END IF  END IF  LET BUTTON$ = "GO|QUIT"  LET DEFAULT = 1  CALL TW_DWARN(ICON, MESSAGE$, BUTTON$, DEFAULT, RESULT)  IF RESULT = 1 THEN    LET Z0$ = "Y"    DECLARE DEF TW_WMAKE          !DEFINITION OF F'n to open a window    LET WINDOW2 = TW_WMAKE( "IMMUNE" )    ASK MAX CURSOR NUMLINES, NUMCOLS    IF N < 8192 THEN      IF FN$ = "A" THEN        LET WW = 55/NUMCOLS      ELSE IF FN$ = "S" OR FN$ = "C" THEN        LET WW = 57/NUMCOLS      END IF    ELSE IF N >= 8192 AND N < 16384 THEN      IF FN$ = "A" THEN        LET WW = 57/NUMCOLS      ELSE IF FN$ = "S" OR FN$ = "C" THEN        LET WW = 61/NUMCOLS      END IF    ELSE IF N >= 16384 AND N < 32768 THEN      IF FN$ = "A" THEN        LET WW = 62/NUMCOLS      ELSE IF FN$ = "S" OR FN$ = "C" THEN        LET WW = 66/NUMCOLS      END IF    ELSE IF N >= 32768 THEN      LET WW = .9    END IF    LET HH = 9/NUMLINES    CALL TW_WSET_CURRENT( WINDOW2,.05,.05+WW,.95-HH,.95 )    CALL TW_WSET_VISIBLE( WINDOW2,1 )    CALL TW_WSWITCH( WINDOW2 )  ELSE IF RESULT = 2 THEN    LET Z0$ = "Q"  END IFEND SUB !BOX54!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB BOX57( ZX$, TOTALTIME, CONCORFLAG )  LET ICON = 2  IF ZX$ = "B" THEN    LET MESSAGE$ = "POWER-OF-2 ANALYSIS RUN COMPLETED"  ELSE IF ZX$ = "C" THEN    LET MESSAGE$ = "COMPOSITE ANALYSIS RUN COMPLETED"  ELSE IF ZX$ = "D" THEN    LET MESSAGE$ = "POWER-OF-2 SYNTHESIS RUN COMPLETED"  ELSE IF ZX$ = "E" THEN    LET MESSAGE$ = "COMPOSITE SYNTHESIS RUN COMPLETED"  ELSE IF ZX$ = "F" THEN    IF CONCORFLAG = 1 THEN      LET MESSAGE$ = "CONVOLUTION  RUN COMPLETED"    ELSE IF CONCORFLAG = 0 THEN      LET MESSAGE$ = "CORRELATION  RUN COMPLETED"    END IF  END IF  LET MESSAGE$ = MESSAGE$ & "|" & "   ELAPSED TIME = " & STR$(TOTALTIME) & " SECONDS"  LET BUTTON$ = "GO"  LET DEFAULT = 1  CALL TW_DWARN(ICON, MESSAGE$, BUTTON$, DEFAULT, RESULT)END SUB !BOX57!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB INPUTVALL( MESSAGE$, VALL, SHOWFLAG, FLAG )  DECLARE DEF EVAL  LET FLAG = 0  SET CURSOR "ON"  LET BUTTON$ = " GO |QUIT"  LET DEFAULT = 1  IF SHOWFLAG = 0 THEN    LET TEXT$ = ""  ELSE IF SHOWFLAG = 1 THEN    LET TEXT$ = STR$(VALL)  END IF  DO    CALL TW_DINPUT(MESSAGE$, BUTTON$, DEFAULT, TEXT$, RESULT)    IF RESULT = 1 THEN      WHEN ERROR IN        LET VALL = EVAL(TEXT$)        EXIT DO      USE        CALL MACSYSBEEP(1)      END WHEN    ELSE IF RESULT = 2 THEN      LET FLAG = 1      EXIT DO    END IF  LOOP  SET CURSOR "OFF"END SUB !INPUTVALL!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB BOX40C( MESSAGE$ )  CALL MACSYSBEEP( 1 )  LET ICON = 3  LET BUTTON$ = "GO"  LET DEFAULT = 1  CALL TW_DWARN( ICON, MESSAGE$, BUTTON$, DEFAULT, RESULT )END SUB !BOX40C!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!SUB BOX40D( MESSAGE$ )  LET ICON = 2  LET BUTTON$ = "GO"  LET DEFAULT = 1  CALL TW_DWARN( ICON, MESSAGE$, BUTTON$, DEFAULT, RESULT )END SUB !BOX40D!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!