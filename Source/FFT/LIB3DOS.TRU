    ELSE IF MODE$ = "SAMP" THEN
      PRINT TAB(10+2*W,21);      "บ  PHASE SPECTRUM ................. G  บ"
    END IF

    IF MODE$ = "DISC" THEN
      PRINT TAB(11+2*W,21);      "บ  MOD F SQUARED .................. H  บ"
