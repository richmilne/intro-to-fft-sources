SUB BOX9( TYPE$, SGR$ )

  IF TYPE$ = "PER" THEN
    LET MESSAGE$ = "SELECT TYPE...|TYPE IS CURRENTLY SET AS PERIODIC"
    LET BUTTON$ = "PERIODIC|PULSE|QUIT"
  ELSE IF TYPE$ = "PUL" THEN
    LET MESSAGE$ = "SELECT TYPE...|TYPE IS CURRENTLY SET AS PULSE"
    LET BUTTON$ = "PERIODIC|PULSE|QUIT"
  ELSE
    LET MESSAGE$ = "SELECT TYPE...|TYPE IS CURRENTLY NOT SET"
    LET BUTTON$ = "PERIODIC|PULSE"
  END IF

  LET DEFAULT = 2

  CALL TD_Warn(MESSAGE$, BUTTON$, DEFAULT, RESULT)

  IF RESULT = 1 THEN
    LET SGR$ = "R"
  ELSE IF RESULT = 2 THEN
    LET SGR$ = "L"
  ELSE IF RESULT = 3 THEN
    LET SGR$ = "Q"
  END IF

END SUB !BOX9
