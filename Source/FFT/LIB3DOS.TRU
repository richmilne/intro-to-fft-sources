SUB BOX41( N, FORM$, NU )

  IF FORM$ = "POWEROF2" THEN
    LET MESSAGE$ = STR$(N) & " IS AN EXACT POWER OF 2|IT IS 2 RAISED TO THE POWER " & STR$(NU)
  ELSE IF FORM$ = "PRIME" THEN
    LET MESSAGE$ = STR$(N) & " IS A PRIME NUMBER|YOU SURE ARE DOING IT THE HARD WAY !"
    LET ICON = 1
  END IF

  LET BUTTON$ = "GO"

  LET DEFAULT = 1

  CALL TD_Warn(MESSAGE$, BUTTON$, DEFAULT, RESULT)

END SUB !BOX41
