SUB BOX42( N, Q, R() )

  LET BF$ = ""
  FOR K = 1 TO Q - 1
    LET BF$ = BF$ & STR$( R(K) ) & ", "
  NEXT K
  LET ZF$ = STR$( R(Q) )
  LET GF$ = BF$ & ZF$

  LET MESSAGE$ = STR$(N) & " IS A COMPOSITE NUMBER... |ITS FACTORS ARE " & GF$
  LET BUTTON$ = "GO"
  LET DEFAULT = 1
  CALL TD_Warn(MESSAGE$, BUTTON$, DEFAULT, RESULT)

END SUB !BOX42
