    END IF
  END IF

  CALL PRINTSUB

  LET FRAZ$ = "ณ SELECT OPTION ณ  ?  "
  DO
    LET TORG$  = ORG$
    LET TORG2$ = ORG2$

    LET RSB = 0
    LET RWS = 0
    DO
      IF RWS = 1 THEN SOUND 500,.1

      CALL APROMPT( 19, 29+BB, A$, FRAZ$, ZUP$ )

